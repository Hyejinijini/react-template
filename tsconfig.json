{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@common/*": ["src/common/*"],
      "@layouts/*": ["src/layouts/*"],
      "@ui/*": ["src/ui/*"]
    }
  }
}
